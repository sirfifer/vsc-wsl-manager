# VSC WSL Manager Configuration Template
# Copy this file to .env.local and update with your values

# GitHub Configuration
GITHUB_USERNAME=your-github-username
GITHUB_TOKEN=your-github-token-for-releases

# VS Code Marketplace Configuration
VSCE_PUBLISHER=your-publisher-name
VSCE_TOKEN=your-marketplace-token

# Development Settings
NODE_ENV=development
DEBUG=false
VERBOSE_LOGGING=false

# Optional: Custom paths
# CUSTOM_ICON_PATH=/path/to/custom/icon.png
# CUSTOM_BUILD_DIR=/path/to/build/output

# Testing Configuration
SKIP_TESTS=false
TEST_TIMEOUT=30000

# Security Settings
ENABLE_SECURITY_LOGGING=false
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=10